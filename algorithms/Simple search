
import java.util.*;


class TestClass {
    public static void main(String args[] ) throws Exception {
       Scanner s1 = new Scanner(System.in);
       int n = s1.nextInt();
       int ar[] = new int[n];
        for(int i=0;i<n;i++)
       {
           ar[i] = s1.nextInt();
       }
       int k = s1.nextInt();
      
       for(int i=0;i<n;i++)
       {
           if(ar[i]==k)
           {System.out.println(i);
           break;}
       }
    }
}

